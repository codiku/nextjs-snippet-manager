generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

enum Language {
  python
  javascript
  java
  csharp
  php
  ruby
  swift
  kotlin
  c
  cpp
  bash
  css
  nextjs
  nodejs
  react
  rust
  typescript
  html
}

model Snippet {
  id       Int      @id @default(autoincrement())
  title    String
  content  String
  language Language
  userId   String
}
